# BMed Prototype Environment Configuration
# Copy this file to .env and fill in your actual values

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Authentication Mode
# Options: simple (for development), google (for production)
REACT_APP_AUTH_MODE=simple

# Development Flags
REACT_APP_DEBUG=false
REACT_APP_DEBUG_AUTH=false
REACT_APP_ENABLE_AUTH_SWITCHING=false
REACT_APP_ENABLE_CLOUD_UPLOADS=true

# Cloud Functions
REACT_APP_GCF_NLP_URL=https://us-central1-your-project.cloudfunctions.net/nlp_processor_gcf
